<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - SistemaDeLogistica</title>
    <link rel="stylesheet" href="~/css/site.css" />
    <script src="~/js/site.js"></script>
</head>
<body>
    <header>
        <nav>
            <a asp-controller="Home" asp-action="Index">Inicio</a> |
            <a asp-controller="Transferencias" asp-action="Create">Nueva Transferencia</a>
        </nav>
    </header>
    <div class="container">
        @RenderBody()
    </div>
    <footer>
        <p>&copy; @DateTime.Now.Year - SistemaDeLogistica</p>
    </footer>
</body>
</html>